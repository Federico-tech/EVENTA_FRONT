{
  "expo": {
    "name": "eventa App",
    "slug": "eventaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logos/GradientBlueLogo.png",
    "scheme": "com.eventa.eventaApp",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./src/assets/logos/BlueLogo.png"
    },
    "splash": {
      "image": "./src/assets/logos/SplashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.eventa.eventaApp"
    },
    "android": {
      "package": "com.eventa.eventaApp",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./src/assets/logos/BlueLogo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "65743897-1a43-4017-a86d-bca502098077"
      }
    }
  }
}
